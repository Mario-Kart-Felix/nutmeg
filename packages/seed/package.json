{
  "name": "@nutmeg/seed",
  "version": "0.17.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Nutmeg Web Component Seed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/nutmeg.git"
  },
  "author": {
    "name": "Abraham Williams",
    "email": "abraham@abrah.am",
    "url": "https://abrah.am"
  },
  "homepage": "https://nutmeg.tools",
  "bugs": {
    "url": "https://github.com/abraham/nutmeg/issues"
  },
  "main": "dist/seed.js",
  "module": "dist/seed.js",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "build": "tsc --project tsconfig.production.json && webpack",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "karma start karma.config.js",
    "watch": "tsc --watch & webpack --watch"
  },
  "keywords": [
    "web-components",
    "webcomponents",
    "shadow-dom",
    "shadowdom",
    "lit-html",
    "nutmeg",
    "typescript"
  ],
  "dependencies": {
    "@abraham/reflection": "0.7.0",
    "lit-html": "1.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/mocha": "8.0.3",
    "@types/node": "10.17.44",
    "@types/webpack": "4.41.25",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "chai": "4.2.0",
    "karma": "5.2.3",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.1.0",
    "karma-mocha": "2.0.1",
    "karma-sinon": "1.0.5",
    "karma-typescript": "5.2.0",
    "karma-typescript-es6-transform": "5.2.0",
    "mocha": "8.1.3",
    "sinon": "9.2.1",
    "typescript": "4.0.5",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "5.4.0",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-cli": "4.2.0"
  },
  "types": "dist/seed.d.ts",
  "directories": {
    "test": "test"
  },
  "unpkg": "dist/seed.min.js"
